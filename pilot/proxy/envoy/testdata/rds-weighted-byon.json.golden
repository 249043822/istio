{
  "virtual_hosts": [
   {
    "name": "hello.default.svc.cluster.local|http",
    "domains": [
     "hello:80",
     "hello",
     "hello.default:80",
     "hello.default",
     "hello.default.svc:80",
     "hello.default.svc",
     "hello.default.svc.cluster:80",
     "hello.default.svc.cluster",
     "hello.default.svc.cluster.local:80",
     "hello.default.svc.cluster.local",
